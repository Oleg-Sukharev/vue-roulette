    </div>\n          <p class=\"congratulation__title\">{{data.congrats}}</p>\n          <p class=\"congratulation__text\">{{data.wherePrize}}</p>\n        </div>\n      </div>\n      <footer class=\"footer\">\n        <action-button tag=\"a\" :href=\"link\" theme=\"dark\" @click=\"actionHandler\">{{data.getPrize}}</action-button>\n      </footer>\n    </div>\n  </div>\n</template>\n\n<script>\nimport ActionButton from '../ActionButton';\n\nexport default {\n  name: 'Popup',\n  components: {\n    ActionButton,\n  },\n  props: {\n    prize: {\n      type: Object,\n      required: true,\n    },\n    data:{\n      type: Object,\n      required: true,\n    }\n  },\n  data() {\n    return {\n      link: process.env.VUE_APP_LANDING_PAGE,\n    };\n  },\n  methods: {\n    actionHandler() {\n      this.$emit('next');\n    },\n  },\n};\n</script>\n\n<style scoped>\n  /* Popup Wrapper */\n  .popup-wrapper {\n    position: absolute;\n    top: 0;\n    left: 0;\n    right: 0;\n    bottom: 0;\n    z-index: 1001;\n  }\n\n  /* Popup */\n  .popup {\n    max-width: 85%;\n    margin: 50% auto 0 auto;\n    border-radius: 1rem;\n    background-color: #96B9FF;\n    transform: translateY(-25%);\n  }\n\n  /* header */\n  .header {\n    position: relative;\n    padding: 1rem;\n    display: grid;\n    grid-template-columns: 1fr auto;\n  }\n\n  .popup__body {\n    text-align: center;\n  }\n\n  /* Prize Description Title */\n  .prize-description__title {\n    margin-top: 0;\n    margin-bottom: 10px;\n    color: #FFFFFF;\n    text-transform: uppercase;\n  }\n  @media (max-width: 599.99px){\n    .prize-description__title {\n      font-size: 1.1rem;\n    }\n  }\n\n  /* Prize Description Name */\n  .prize-description__name {\n    margin: 0;\n    font-weight: 600;\n    color: #1C0057;\n    text-transform: uppercase;\n  }\n  @media (max-width: 599.99px) {\n    .prize-description__name {\n      font-size: 1.4rem;\n    }\n  }\n\n  /* Prize Image */\n  .prize-image {\n    width: 100px;\n    height: 80px;\n    position: relative;\n  }\n\n  .prize-image img {\n    position: absolute;\n    width: 100%;\n    top: -30%;\n    right: 0;\n  }\n\n  .congratulation {\n    max-width: 80%;\n    margin: 3% auto 0;\n    display: grid;\n    align-items: center;\n    text-align: center;\n  }\n\n  /* Congratulation Image */\n  .congratulation__image {\n    display: flex;\n    justify-content: center;\n  }\n  .congratulation__image img {\n    width: 60px;\n  }\n\n  /* Congratulation Title*/\n  .congratulation__title {\n    margin-top: 1rem;\n    margin-bottom: 1rem;\n    font-weight: 600;\n    color: #1C0057;\n    text-transform: uppercase;\n  }\n  @media (max-width: 599.99px) {\n    .congratulation__title {\n      font-size: 1.4rem;\n    }\n  }\n  /* Congratulation Text */\n  .congratulation__text {\n    margin: 0;\n    color: #300057;\n  }\n  @media (max-width: 599.99px) {\n    .congratulation__text {\n      font-size: 1rem;\n    }\n  }\n\n  .footer {\n    padding-top: 1rem;\n    padding-bottom: 1rem;\n  }\n</style>"]}]}