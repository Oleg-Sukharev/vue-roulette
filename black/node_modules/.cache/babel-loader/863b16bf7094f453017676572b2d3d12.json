AA;AACA,YAAA,MAAA,EAAA;AADA,WAAA;;AAGA,aAAA,IAAA;AACA,iBAAA;AACA,YAAA,SAAA,EAAA,KAAA,CAAA,SADA;AAEA,YAAA,MAAA,EAAA;AAFA,WAAA;AANA;AAWA;AAvBA,GAlBA;AA2CA,EAAA,OA3CA,qBA2CA;AACA,QAAA,QAAA,GAAA,OAAA,CAAA,GAAA,CAAA,YAAA;;AAEA,QAAA,QAAA,KAAA,UAAA,EAAA;AACA,WAAA,QAAA,GAAA,QAAA;AACA,KAFA,MAGA,IAAA,QAAA,KAAA,MAAA,EAAA;AACA,WAAA,QAAA,GAAA,IAAA;AACA;AACA,GApDA;AAqDA,EAAA,OAAA,EAAA;AACA,IAAA,YADA,wBACA,UADA,EACA;AACA,WAAA,IAAA,GAAA,UAAA;AACA;AAHA;AArDA,CAAA","sourcesContent":["<template>\n  <div id=\"app\">\n    <div :class=\"['app', `app--${theme}`, {blur: blur}]\">\n      <div class=\"app-vertical-position\">\n        <div class=\"app-horizontal-position\" :style=\"{maxWidth: appState === 0 ? '100%' : '80%'}\">\n          <component\n            :is=\"currentComponent\"\n            v-bind=\"{...componentProps,data}\"/>\n        </div>\n      </div>\n      <div class=\"dummy\"></div>\n      <div class=\"app__winners\">\n        <winners-ribbon\n          :winners=\"winners\"\n          :flip-interval=\"3000\"\n          :data=\"data\"\n          :theme=\"theme\"/>\n      </div>\n    </div>\n    <portal-target name=\"popup\" @change=\"portalHandle\"/>\n  </div>\n</template>\n\n<script>\nimport winners from './assets/winners.json';\nimport questions from './assets/questions.json';\nimport prizes from './assets/prizes.json';\nimport data from './assets/data.json';\n\n\nimport RouletteView from './components/RouletteView';\nimport TestView from './components/TestView';\nimport WinnersRibbon from './components/WinnersRibbon';\n\nconst ROULETTE = 0;\nconst TEST = 1;\n\nexport default {\n  name: 'App',\n  components: {\n    WinnersRibbon,\n    TestView,\n    RouletteView,\n  },\n  data() {\n    return {\n      theme: 'dark',\n      blur: false,\n      appState: ROULETTE,\n      winners,\n      questions,\n      prizes,\n      data,\n    };\n  },\n  computed: {\n    currentComponent() {\n      switch (this.appState) {\n        case ROULETTE:\n          this.theme = 'dark';\n          return 'roulette-view';\n        case TEST:\n          this.theme = 'light';\n          return 'test-view';\n      }\n    },\n    componentProps() {\n      switch (this.appState) {\n        case ROULETTE:\n          return {\n            prizes,\n          };\n        case TEST:\n          return {\n            questions: data.questions,\n            prizes,\n          };\n      }\n    },\n  },\n  mounted() {\n    const APP_MODE = process.env.VUE_APP_MODE;\n\n    if (APP_MODE === 'roulette') {\n      this.appState = ROULETTE;\n    }\n    else if (APP_MODE === 'quiz') {\n      this.appState = TEST;\n    }\n  },\n  methods: {\n    portalHandle(newContent) {\n      this.blur = newContent;\n    }\n  }\n}\n</script>\n\n<style>\n.app {\n  position: relative;\n  height: 100vh;\n  width: 100vw;\n  display: grid;\n  grid-template-rows: minmax(auto, 550px) 1fr 110px;\n  overflow: auto;\n}\n\n.app--dark {\n  background-color: #18004D;\n}\n\n.app--light {\n  background-color: #E5E5E5;\n}\n\n/* App Winners */\n.app__winners  {\n  max-width: 570px;\n  margin: 0 auto;\n  overflow-y: hidden;\n}\n@media (min-width: 600px) {\n  .app__winners {\n    width: 100%;\n  }\n}\n@media (max-width: 599.99px) {\n  .app__winners {\n    width: 80%;\n  }\n}\n\n/* App Test */\n.app-horizontal-position {\n  margin-left: auto;\n  margin-right: auto;\n  display: grid;\n  height: 100%;\n}\n\n@media (max-width: 599.99px) {\n  .app-horizontal-position {\n    max-height: 570px;\n  }\n}\n\n.app-vertical-position {\n  position: relative;\n  margin-top: 10%;\n}\n\n.blur {\n  filter: blur(2px);\n}\n\n.fade-enter-active, .fade-leave-active {\n  opacity: 100%;\n  transition: opacity 1s;\n}\n.fade-enter, .fade-leave-to {\n  opacity: 0;\n}\n\n.dummy {\n  margin-top: 10px;\n}\n</style>\n"],"sourceRoot":"src"}]}